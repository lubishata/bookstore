{
  "name": "bookstore-service",
  "version": "1.0.0",
  "description": "bookstore backend service",
  "main": "./build/src/main.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "npx concurrently \"npm:watch-ts\" \"npm:watch-js\"",
    "watch-ts": "npx tsc --watch --preserveWatchOutput",
    "watch-js": "npx nodemon",
    "start": "ts-node src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lubishata/bookstore.git"
  },
  "keywords": [
    "messaging",
    "database"
  ],
  "author": "the team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lubishata/bookstore/issues"
  },
  "homepage": "https://github.com/lubishata/bookstore#readme",
  "devDependencies": {
    "@nestjs/platform-express": "^10.0.5",
    "@types/node": "^14.18.51",
    "@types/nodemon": "^1.19.2",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.2.6",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "gts": "^3.1.1",
    "log4js": "^6.9.1",
    "nodemon": "^3.0.1",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.0"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/graphql": "^12.0.8",
    "graphql": "^16.7.1"
  }
}
